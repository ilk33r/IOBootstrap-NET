    <Folder Include="Core\Controllers\" />
    <Folder Include="Core\System\" />
    <Folder Include="Core\Database\" />
    <Folder Include="Common\Entities\Clients\" />
    <Folder Include="Common\Entities\AutoIncrements\" />
