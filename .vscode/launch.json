{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "IOBootstrap.Net.MW.Application",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildIOBootstrapNetMWApplication",
            "program": "${workspaceFolder}/MW/Application/bin/Debug/net6.0/IOBootstrap.NET.MW.Application.dll",
            "args": [],
            "cwd": "${workspaceFolder}/MW/Application",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "^\\s*Now listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://localhost:5004"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "IOBootstrap.Net.Default.Application",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildIOBootstrapNetDefaultApplication",
            "program": "${workspaceFolder}/Default/Application/bin/Debug/net6.0/IOBootstrap.NET.Default.Application.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Default/Application",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "^\\s*Now listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://localhost:5000"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "IOBootstrap.Net.Functions.PushNotifications",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildIOBootstrapNetFunctionsPushNotifications",
            "program": "${workspaceFolder}/Functions/PushNotifications/bin/Debug/net6.0/IOBootstrap.Net.Functions.PushNotification.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Functions/PushNotifications",
            "stopAtEntry": false,
            "requireExactSource": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "IOBootstrapNetBatch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildIOBootstrapNetBatch",
            "program": "${workspaceFolder}/Batch/bin/Debug/netcoreapp6.0/IOBootstrap.NET.Batch.dll",
            "args": [
                "dev",
                "${workspaceFolder}/SevenK/Application"
            ],
            "cwd": "${workspaceFolder}/Batch",
            "stopAtEntry": false,
            "requireExactSource": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "Backoffice.UI",
            "request": "launch",
            "cwd": "${workspaceFolder}/Application/Backoffice.UI",
            "runtimeArgs": [
                "start"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "name": "IOBootstrapNetApplicationPublish",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "publishIOBootstrapNetApplication",
            "program": "${workspaceFolder}/Application/bin/Debug/netcoreapp3.1/IOBootstrap.NET.Application.dll",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Release",
                "ASPNETCORE_URLS": "http://localhost:5000"
            }
        },
        {
            "name": "Attach to .NET Functions",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:azureFunctions.pickProcess}"
        }
    ]
}